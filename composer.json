{
  "name": "andrew-svirin/resource-crawler-bundle",
  "description": "Crawler for any sort of resources.",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^5.4"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.13",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "sebastian/phpcpd": "^6.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "AndrewSvirin\\ResourceCrawlerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndrewSvirin\\ResourceCrawlerBundle\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Svirin"
    }
  ],
  "archive": {
    "exclude": [
      "/infrastructure",
      "/tests",
      "Makefile"
    ]
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
